{
  "name": "plus-pro-components",
  "version": "0.0.0",
  "description": "Page level components developed based on Element Plus.",
  "author": "xiaofei",
  "keywords": [
    "plus-pro-components",
    "element-plus",
    "Page level components",
    "component library",
    "ui framework",
    "ui",
    "vue",
    "echarts"
  ],
  "homepage": "https://github.com/plus-pro-components/plus-pro-components",
  "bugs": {
    "url": "https://github.com/plus-pro-components/plus-pro-components/issues"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "module": "es/index.mjs",
  "types": "es/index.d.ts",
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.mjs",
      "require": "./lib/index.js"
    },
    "./es": "./es/index.mjs",
    "./lib": "./lib/index.js",
    "./es/*.mjs": "./es/*.mjs",
    "./es/*": "./es/*.mjs",
    "./lib/*.js": "./lib/*.js",
    "./lib/*": "./lib/*.js",
    "./*": "./*"
  },
  "unpkg": "index.js",
  "jsdelivr": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plus-pro-components/plus-pro-components.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "sideEffects": [
    "dist/*"
  ],
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "@types/lodash-es": "^4.17.6",
    "element-plus": "^2.3.4",
    "dayjs": "^1.11.3",
    "lodash-es": "^4.17.21",
    "sortablejs": "^1.15.0"
  },
  "devDependencies": {
    "@types/node": "*",
    "@types/sortablejs": "^1.15.1",
    "vue": "^3.2.37",
    "@plus-pro-components/utils": "^0.0.9"
  },
  "browserslist": [
    "> 1%",
    "not ie 11",
    "not op_mini all"
  ]
}
